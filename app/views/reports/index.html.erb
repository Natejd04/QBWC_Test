<div id="page-wrapper" style="min-height: 629px;">
    
      <div class="row">
        <div class="col-lg-12">
            <div class="panel-body">
            <%= render_report 'my_users' do |report| %>     
                <%= report.form do |f| %>
                    <div class="pull-right">
                        <%= f.date_range :c_date %>
                    </div>
                    <%= f.multi_autocomplete :customer, placeholder: 'Customer...' %>
                <% end %>
            <% end %>
        </div>
    </div>
</div>