<table class="inv">
    <th colspan="16">Masters/Caddies - WDS</th>
    <tr>
        <%= render :partial => 'wdsmast' %> 
    </tr>
    <tr>
        <%= render :partial => 'wdscad' %>
    </tr>
    <tr>
        <%= render :partial => 'wdssomast' %>
    </tr>
</table>


<h3>Ship from WDS</h3>
<% i=0 %>

<table>
    <th></th>
    <th>Customer</th>
    <th>Ship Date</th>
    <th>Shipping Method</th>
    <th>Attachment</th>
    <th>Acknowledged</th>
    <% @orders.each do |order| %>
    <tr>
        <td><%= i = i+1 %> .  </td>
        <td><%= link_to order.customer.name, order %><br /></td>
        <td><%= order.c_ship.strftime("%m/%d/%Y") %></td>
        <td><%= order.c_via %></td>
        <td><%= link_to "Download", order.docs.url(:docs, false) %></td>
        <td><%= order.c_ack %></td>
        <% end %>
    </tr>
</table>

