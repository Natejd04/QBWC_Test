<h3>Ship From Zing</h3>
<table>
    <th>Customer</th>
    <th>Ship Date</th>
    <th>Shipping Method</th>
    <th>Attachment</th>
    <th>Acknowledged</th>
    <% @order_zing.each do |order| %>
    <tr>
        <td><%= link_to order.customer.name, order %><br /></td>
        <td><%= order.c_ship.strftime("%m/%d/%Y") %></td>
        <td><%= order.c_via %></td>
        <td><%= order.c_ack %></td>
        <% end %>
    </tr>
</table>
